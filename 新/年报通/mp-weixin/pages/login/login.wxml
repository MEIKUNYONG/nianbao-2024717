<view><image class="pageBg" src="/static/img/loginBg.png" mode="widthFix"></image><image class="logo" src="{{config.wxlogo}}" mode="widthFix"></image><view class="name">{{miniName}}</view><block wx:if="{{isCheck}}"><button class="loginBtn" open-type="getPhoneNumber" data-event-opts="{{[['getphonenumber',[['getPhone',['$event']]]]]}}" bindgetphonenumber="__e">手机号快捷登录</button></block><block wx:else><button data-event-opts="{{[['tap',[['phone',['$event']]]]]}}" class="loginBtn" bindtap="__e">手机号快捷登录</button></block><view class="line"><checkbox-group data-event-opts="{{[['change',[['checkchange',['$event']]]]]}}" bindchange="__e"><checkbox value="0"></checkbox></checkbox-group><view>登录即同意<text class="tit" data-id="0" data-event-opts="{{[['tap',[['toPace',['$event']]]]]}}" bindtap="__e">{{config.agreement_title}}</text>和<text class="tit" data-id="1" data-event-opts="{{[['tap',[['toPace',['$event']]]]]}}" bindtap="__e">{{config.privacy_title}}</text></view></view><block wx:if="{{showPace}}"><view class="mask"></view></block><block wx:if="{{showPace}}"><view class="pane"><view class="head"><view class="title">{{agreement_title}}</view><view data-event-opts="{{[['tap',[['closePace',['$event']]]]]}}" class="iconfont icon-cuo _i" bindtap="__e"></view></view><scroll-view class="cont" scroll-y="true"><rich-text nodes="{{agreement}}"></rich-text></scroll-view><view data-event-opts="{{[['tap',[['closePace',['$event']]]]]}}" class="btn" bindtap="__e">同意本协议</view></view></block></view>